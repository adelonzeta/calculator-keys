{
  "name": "calculator-keys",
  "version": "0.0.1-3",
  "description": "Calculator Keys Javascript Library.",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "files": [
    "lib"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.ts$": "ts-jest"
    },
    "testRegex": "/src/.*\\.test\\.ts$"
  },
  "keywords": [
    "scientific",
    "calculator",
    "typescript",
    "javascript",
    "library"
  ],
  "author": "Adelon Zeta <hello@adelonzeta.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adelonzeta/scientific-calculator.git"
  },
  "bugs": {
    "url": "https://github.com/adelonzeta/scientific-calculator/issues"
  },
  "homepage": "https://github.com/adelonzeta/scientific-calculator#readme",
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.5",
    "typescript": "^3.2.2"
  }
}
